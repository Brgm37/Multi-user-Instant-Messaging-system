@startuml

class User
{
    + uId: UInt?
    + username: String
    + password: Password
}

class Password
{
    + value: String
    + matches(password: String): Boolean
    - PASSWORD_MIN_LENGTH: Int
    - PASSWORD_PATTERN: String
    + isValidPassword(password: String): Boolean
}

class UserInfo
{
    + uId: UInt
    + username: String
}

class UserInvitation
{
    + inviterId: UInt
    + expirationDate: Timestamp
    + invitationCode: UUID
    + isExpired: Boolean
}

User ..> Password

@enduml